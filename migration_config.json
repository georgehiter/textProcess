{
  "strategy": "gradual",
  "priority": "high",
  "enabled": true,
  "auto_migration": false,
  "initial_traffic_percentage": 0.25,
  "max_traffic_percentage": 1.0,
  "traffic_increment": 0.1,
  "traffic_increment_interval": 3600,
  "schedule": {
    "start_time": "00:00",
    "end_time": "23:59",
    "timezone": "UTC",
    "days_of_week": [
      0,
      1,
      2,
      3,
      4,
      5,
      6
    ],
    "max_concurrent_migrations": 3
  },
  "monitoring": {
    "enabled": true,
    "metrics_interval": 60,
    "alert_enabled": true,
    "alert_thresholds": {
      "error_rate_threshold": 0.05,
      "response_time_threshold": 2.0,
      "success_rate_threshold": 0.95,
      "rollback_threshold": 0.1
    },
    "log_level": "INFO",
    "log_file": "migration.log"
  },
  "auto_rollback_enabled": true,
  "rollback_on_error": true,
  "rollback_threshold": 0.05,
  "performance_degradation_threshold": 0.2,
  "max_response_time_increase": 1.0,
  "backup_enabled": true,
  "backup_retention_days": 30,
  "data_validation_enabled": true,
  "notification_enabled": false,
  "notification_channels": [],
  "notification_events": [
    "migration_started",
    "migration_completed",
    "migration_failed",
    "rollback_triggered"
  ]
}